<!DOCTYPE html>
<html>
<head>
  <title>Oto & Oro | <%= yield(:title) %></title>
 
  <%= render 'layouts/stylesheet' %>
  <%= csrf_meta_tags %>

</head>
<body>


<div class="container">
    <%= render 'layouts/header' %>
    <section class="round">
    <% flash.each do |key, value| %>
    <div class="flash_<%= key %>">
    	<%= value %>
    </div> <br />
    <% end %>
      <%= yield %>
      <img src="/hearing3.jpg", class="imge">
    </section>
    
    <%= render 'layouts/footer' %>
    <!--<%= debug(params) if Rails.env.development? %> --> 
</div>

</body>
</html>
